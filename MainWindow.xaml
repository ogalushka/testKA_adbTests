<Window x:Class="Keyapp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Keyapp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
    <StackPanel Orientation="Vertical" Margin="10">
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
            <TextBox MinWidth="350" Margin="10" Name="Querry"/>
            <Button Content="Perform a search" Margin="10" Click="Button_Click" Name="ExecuteButton"/>
        </StackPanel>
        <TextBlock Margin="10" TextAlignment="Center" Name="OutputText"/>
    </StackPanel>
</Window>
